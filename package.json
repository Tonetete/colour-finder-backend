{
  "name": "colours-finder-backend",
  "version": "1.0.0",
  "description": "backend for colours finder app",
  "main": "index.js",
  "scripts": {
    "serverless": "./node_modules/.bin/serverless",
    "package": "yarn serverless package",
    "remove-stack": "./node_modules/.bin/serverless remove",
    "build": "rm -rf .build && ./node_modules/.bin/tsc --project ./tsconfig.json",
    "deploy": "yarn build && serverless deploy -v",
    "test": "./node_modules/.bin/jest -c jest.config.ts",
    "test:coverage": "./node_modules/.bin/jest -c jest.config.ts --coverage"
  },
  "author": "antonio rivas",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "serverless": "^2.35.0",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "aws-sdk": "^2.888.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3"
  }
}
